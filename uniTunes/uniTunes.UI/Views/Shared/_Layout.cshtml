<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="title" content="UniTunes" />
    <meta name="author" content="Felipe Coelho Machado, Guilherme Marmitt, Gustavo Larsen, Helton Batista">    
    <meta name="copyright" content="© UniTunes" />
    <meta name="reply-to" content="fcm1990@gmail.com">
    <link rel="shortcut icon" href="@Url.Content("~/Content/Images/network.png")" type="image/x-icon" />
    <title>UniTunes</title>
    @Styles.Render("~/bundles/css")
</head>
<body>
    
    @Html.Partial("_Header")

    <div class="container">

        @RenderSection("messages", required: false)

        @RenderBody()
    </div>
    
    @Html.Partial("_Footer")

    @Scripts.Render("~/bundles/js")
    @RenderSection("scripts", required: false)
</body>
</html>

